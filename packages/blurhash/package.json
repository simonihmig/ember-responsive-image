{
  "name": "@ember-responsive-image/blurhash",
  "version": "3.4.0",
  "description": "Automatically generate resized images at build-time, optimized for the responsive web, and using components to render them easily as <picture> elements.",
  "repository": "https://github.com/kaliber5/ember-responsive-image",
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "keywords": [
    "ember-addon"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "rollup --config --watch",
    "test": "echo 'No tests yet'",
    "prepack": "rollup --config"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0",
    "blurhash": "^2.0.4"
  },
  "peerDependencies": {
    "@ember-responsive-image/webpack": "^3.4.0",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/preset-typescript": "7.23.3",
    "@ember-responsive-image/webpack": "^3.4.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@tsconfig/ember": "1.1.0",
    "@types/sharp": "0.31.1",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "concurrently": "8.2.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-node": "11.1.0",

    "eslint-plugin-prettier": "4.2.1",
    "rollup": "2.79.1",
    "rollup-plugin-node-externals": "5.1.3",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-ts": "3.4.5",
    "sharp": "0.31.3",
    "typescript": "5.2.2",
    "webpack": "5.89.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "public-assets": {
      "./dist/index.js": "/assets/blurhash.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
