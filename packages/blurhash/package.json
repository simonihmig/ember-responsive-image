{
  "name": "@ember-responsive-image/blurhash",
  "version": "1.0.0-beta.0",
  "description": "Add support for BlurHash encoded LQIP to ember-responsive-images",
  "repository": "https://github.com/simonihmig/ember-responsive-image",
  "license": "MIT",
  "author": "Simon Ihmig <simon.ihmig@gmail.com>",
  "keywords": [
    "ember-addon"
  ],
  "files": [
    "declarations",
    "dist"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "rollup --config --watch",
    "test": "echo 'No tests yet'",
    "prepack": "rollup --config"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.7",
    "blurhash": "^2.0.4"
  },
  "peerDependencies": {
    "@ember-responsive-image/webpack": "^1.0.0-beta.0",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/preset-typescript": "7.24.1",
    "@ember-responsive-image/webpack": "workspace:*",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@tsconfig/ember": "3.0.6",
    "@types/node": "20.12.7",
    "@types/sharp": "0.32.0",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-n": "17.2.1",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "rollup": "4.16.2",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-node-externals": "6.1.2",
    "sharp": "0.33.3",
    "typescript": "5.4.5",
    "webpack": "5.91.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "public-assets": {
      "./dist/index.js": "/assets/blurhash.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
